{
  "extends": [
    "tslint-config-standard"
  ],
  "rules": {
    "space-before-function-paren": false,
    "trailing-comma": [
      true, {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "typeLiterals": "ignore",
          "functions": "ignore"
        },
        "singleline": "never"
      }
    ]
  }
}
